        "n = 5000\n", 
        "for i in range(n):\n", 
        "\tdata.append([i+1, (i+1)*10])\n", 
        "data = array(data)\n", 
        "# drop time\n", 
        "data = data[:, 1]\n", 
        "print(data.shape)"
